{
  "name": "Paya.MuniBilling.IVR.Wrapper",
  "version": "0.0.15-0",
  "description": "Collection of Lambdas used by the MuniBilling IVR.",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "build:status": "tsc --skipLibCheck -p src/status",
    "build:generateToken": "tsc --skipLibCheck -p src/generateToken",
    "build:merchant": "tsc --skipLibCheck -p src/merchant",
    "build:account": "tsc --skipLibCheck -p src/account",
    "build:convenienceFee": "tsc --skipLibCheck -p src/convenienceFee",
    "build:saveTransaction": "tsc --skipLibCheck -p src/saveTransaction",
    "postbuild": "run-s build:**",
    "lint": "eslint src/**/*.{js,ts} --color",
    "lint:status": "eslint src/status/*.ts --color",
    "lint:generateToken": "eslint src/generateToken/*.ts --color",
    "lint:merchant": "eslint src/merchant/*.ts --color",
    "lint:account": "eslint src/account/*.ts --color",
    "lint:convenienceFee": "eslint src/convenienceFee/*.ts --color",
    "lint:saveTransaction": "eslint src/saveTransaction/*.ts --color",
    "postlint": "run-s v:**",
    "test": "jest --silent -i",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy": "serverless deploy",
    "install:status": "cd src/status && npm i",
    "install:generateToken": "cd src/generateToken && npm i",
    "install:merchant": "cd src/merchant && npm i",
    "install:account": "cd src/account && npm i",
    "install:convenienceFee": "cd src/convenienceFee && npm i",
    "install:saveTransaction": "cd src/saveTransaction && npm i",
    "install:authorizer": "cd src/authorizer && npm i",
    "postinstall": "run-s install:**",
    "sls:deploy": "sls deploy",
    "sls:package": "sls package",
    "sls:rollback": "sls rollback"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@aws-sdk/client-secrets-manager": "^3.54.1",
    "@dynamic-mapper/mapper": "^1.10.4",
    "axios": "^1.3.5",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@nrwl/eslint-plugin-nx": "^14.4.3",
    "@types/argparse": "^2.0.1",
    "@types/aws-lambda": "^8.10.91",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.5.9",
    "@types/seedrandom": "^3.0.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "env-cmd": "^10.1.0",
    "eslint-plugin-jsdoc": "35.4.6",
    "eslint-plugin-sonarjs": "0.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "npm-run-all": "^4.1.5",
    "serverless": "^3.38.0",
    "serverless-bundle": "^5.4.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^9.0.0",
    "serverless-offline-ssm": "^6.2.0",
    "typescript": "^4.7.4"
  }
}
